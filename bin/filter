#!/usr/bin/env python3
import string

class StampExc_No_T(Exception):
    pass
class Stamp(str):
    def __init__(self,*a,**b):
        self.validate()
        tpos = self.upper().find('T')
        if tpos == -1: raise StampExc_No_T

    def validate(self):
        pass
a=Stamp('helilo')
exit()
print(a.upper())
def normed4stamp(stamp):
    digits = string.digits + '_'
    stamp=stamp.upper()
    left, right = stamp.split('T')
    left = ''.join( char for char in left if char in digits )
    right = ''.join( char for char in right if char in digits )
    assert len(left)==8
    assert len(right)==6
    return left + 'T' + right

print(normed4stamp('20010202t__1234'))
#assert normed4stamp('2000-01-02-t-10:11:12') == '20000102T101112'
def stdin_lines():
    while True:
        try:
            yield input()
        except EOFError:
            break
    
def map_stdin(fn=lambda x:x):
    try:
        while True:
            a=input()
            print('xxx', a)
    except EOFError:
        pass

